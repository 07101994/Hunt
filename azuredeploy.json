{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": { 
	    "siteName": {
	        "type": "string",
	        "defaultValue": "[concat('site', uniqueString(resourceGroup().id))]"
	    }
	},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/bc1b70ab-d65f-4c57-9f64-33b21d29b91c/resourcegroups/huntappresourcegroup/providers/Microsoft.Cdn/profiles/huntapp'.",
            "type": "Microsoft.Cdn/profiles",
            "sku": {
                "name": "Standard_Verizon"
            },
            "name": "[concat(parameters('siteName'), '-cdn')]",
            "apiVersion": "2016-04-02",
            "location": "[resourceGroup().location]",
            "tags": {},
            "scale": null,
            "properties": {
                "provisioningState": "Succeeded",
                "resourceState": "Active"
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/bc1b70ab-d65f-4c57-9f64-33b21d29b91c/resourceGroups/huntappresourcegroup/providers/Microsoft.CognitiveServices/accounts/huntappvision'.",
            "type": "Microsoft.CognitiveServices/accounts",
            "sku": {
                "name": "F0"
            },
            "kind": "ComputerVision",
            "name": "[concat(parameters('siteName'), '-vision')]",
            "apiVersion": "2016-02-01-preview",
            "location": "[resourceGroup().location]",
            "scale": null,
            "properties": {},
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/bc1b70ab-d65f-4c57-9f64-33b21d29b91c/resourceGroups/huntappresourcegroup/providers/Microsoft.CognitiveServices/accounts/huntappcontentmoderator'.",
            "type": "Microsoft.CognitiveServices/accounts",
            "sku": {
                "name": "F0"
            },
            "kind": "ContentModerator",
            "name": "[concat(parameters('siteName'), '-content-moderator')]",
            "apiVersion": "2016-02-01-preview",
            "location": "[resourceGroup().location]",
            "scale": null,
            "properties": {},
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/bc1b70ab-d65f-4c57-9f64-33b21d29b91c/resourceGroups/huntappresourcegroup/providers/Microsoft.DocumentDB/databaseAccounts/huntapp'.",
            "type": "Microsoft.DocumentDB/databaseAccounts",
            "kind": "GlobalDocumentDB",
            "name": "[concat(parameters('siteName'), '-documentDb')]",
            "apiVersion": "2015-04-08",
            "location": "[resourceGroup().location]",
            "tags": {
                "defaultExperience": "DocumentDB"
            },
            "scale": null,
            "properties": {
                "databaseAccountOfferType": "Standard",
                "consistencyPolicy": {
                    "defaultConsistencyLevel": "Session",
                    "maxIntervalInSeconds": 5,
                    "maxStalenessPrefix": 100
                },
                "name": "[concat(parameters('siteName'), '-database')]"
            },
            "dependsOn": []
        }
	],
    "outputs": {  }
}